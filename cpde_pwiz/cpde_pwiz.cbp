				<Option object_output=".cmp/gcc/obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc_generic" />
				<Compiler>
					<Add option="-std=c++11" />
					<Add option="-g" />
					<Add option="-W" />
					<Add option="-fPIC" />
					<Add option="-fexceptions" />
					<Add option="`$(#wx.config) --cxxflags --debug`" />
					<Add option="-DNOPCH" />
					<Add option="-D_DEBUG" />
				</Compiler>
				<Linker>
					<Add option="`$(#wx.config) --libs std,aui  --static --debug`" />
